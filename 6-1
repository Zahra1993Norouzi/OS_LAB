#include <stdio.h>

void compute_prefix_sum(int *input_array, int *output_array, int n) {
    output_array[0] = input_array[0]; 
    for (int i = 1; i < n; i++) {
        output_array[i] = output_array[i - 1] + input_array[i]; 
    }
}

int main() {
    int input_array[] = {1, 2, 3, 4, 5, 6};
    int n = sizeof(input_array) / sizeof(input_array[0]); 
    int output_array[n]; 

    compute_prefix_sum(input_array, output_array, n);

    printf("Input Array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", input_array[i]);
    }
    printf("\n");

    printf("Prefix Sum: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", output_array[i]);
    }
    printf("\n");

    return 0;
}
